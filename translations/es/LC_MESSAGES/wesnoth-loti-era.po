msgstr "capaz de quemar a enemigos en aldeas con 90% de probabilidad"

#. [advancement], id=torch3
#: LotI_Era/units/Goblin_Ravager.cfg:289
#: LotI_Era/units/mainline/Marauder.cfg:37
msgid "incinerating with torch"
msgstr "incinerando con antorcha"

#. [advancement], id=net-denial
#: LotI_Era/units/Goblin_Ravager.cfg:308
msgid "able to throw nets at enemies in villages with 90% chance"
msgstr "capaz de lanzar redes a enemigos en aldeas con 90% de probabilidad"

#. [unit_type], id=Goblin Trainer LotI
#: LotI_Era/units/Goblin_Trainer.cfg:7
msgid "Goblin Trainer"
msgstr "Entrenador goblin"

#. [trait], id=loyal
#: LotI_Era/units/Goblin_Trainer.cfg:11
msgid "loyal"
msgstr "leal"

#. [trait], id=loyal
